      "metadata": {}
    },
    {
      "cell_type": "code",
      "source": "region_count=df.groupby('Region')['Count'].sum()\nplt.pie(region_count,labels=region_count.index)\nplt.title('Percentage of Pixels for Presumed Vegetation Fires by Region')\nplt.legend([(i,round(k/region_count.sum()*100,2)) for i,k in zip(region_count.index, region_count)])\nplt.axis('equal')\nplt.show()",
      "metadata": {
        "trusted": true
      },
      "outputs": [
        {
          "output_type": "display_data",
          "data": {
            "text/plain": "<Figure size 640x480 with 1 Axes>",
            "image/png": ""
